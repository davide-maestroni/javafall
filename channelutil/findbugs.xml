<FindBugsFilter>
    <Match>
        <!-- Too many false positives... -->
        <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT"/>
    </Match>
    <Match>
        <Class name="com.github.dm.jrt.channel.util.JoinBuilder$JoinOutputConsumer"/>
        <Method name="onOutput"/>
        <Bug pattern="UC_USELESS_OBJECT"/>
    </Match>
    <Match>
        <Class name="com.github.dm.jrt.channel.util.JoinBuilder$JoinOutputConsumer"/>
        <Method name="flush"/>
        <Bug pattern="UC_USELESS_OBJECT"/>
    </Match>
    <Match>
        <Class name="com.github.dm.jrt.channel.util.RepeatedChannel"/>
        <Method name="next"/>
        <Bug pattern="IT_NO_SUCH_ELEMENT"/>
    </Match>
</FindBugsFilter>
